{
  "manifest_version": 3,
  "name": "quiz helper by tengocj",
  "description": "Quiz monitor extension.",
  "version": "0.2.0",
  "action": {
    "default_title": "quiz helper by tengocj",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "all_frames": true,
      "match_about_blank": true,
      "run_at": "document_idle"
    }
  ]
}
